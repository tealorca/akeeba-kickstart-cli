{
    "name": "tealorca/akeeba-kickstart-cli",
    "description": "The Laravel Zero Framework.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli", "akeeba-kickstart", "akeeba-kickstart-cli"],
    "homepage": "https://tealorca.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/tealorca/akeeba-kickstart-cli/issues",
        "source": "https://github.com/tealorca/akeeba-kickstart-cli"
    },
    "authors": [
        {
            "name": "Daniel Felix",
            "email": "danielfelix1995@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "fabpot/goutte": "^4.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0",
        "laravel-zero/framework": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/akeeba-kickstart-cli"]
}
